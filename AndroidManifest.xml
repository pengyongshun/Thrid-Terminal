        <activity
            android:name=".face.ui.FaceLoginActivity"
            android:screenOrientation="portrait" />
        <!-- 媒体设置 -->
        <activity
            android:name=".ui.activity.MediaSettingActivity"
            android:configChanges="keyboardHidden|screenSize|orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.ResolutionRatioActivity"
            android:configChanges="keyboardHidden|screenSize|orientation"
            android:screenOrientation="portrait" />
